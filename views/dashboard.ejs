<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Funding Profit INR</title>
    <link rel="stylesheet" href="/css/dashboard.css">
</head>
<body>
    <div class="container">
        <!-- Header -->
        <header class="main-header">
            <h1>Funding Profit INR</h1>
        </header>

        <!-- Button Grid -->
        <div class="button-grid">
            <button class="grid-button" id="btn-symbol">Symbol</button>
            <button class="grid-button" id="btn-spot">SPOT</button>
            <button class="grid-button" id="btn-futures">Futures</button>
            <button class="grid-button" id="btn-amount">Amount (USDT)</button>
            <button class="grid-button" id="btn-takeprofit">Take Profit</button>
            <button class="grid-button" id="btn-go">GO</button>
        </div>

        <!-- Open Positions Section -->
        <section class="open-positions">
            <h2 class="section-title">ALL Open Positions</h2>
            <div class="manual-close-divider">
                <span>Manual Close</span>
            </div>
            
            <div class="positions-content">
                <!-- Future content will go here -->
            </div>
        </section>

        <!-- Logout Link -->
        <div class="footer-actions">
            <a href="/logout" class="logout-link">Logout</a>
        </div>
    </div>

    <script src="/js/dashboard.js"></script>
</body>
</html>